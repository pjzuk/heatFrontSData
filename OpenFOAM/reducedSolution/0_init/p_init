FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      pst;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inside
    {
        type            zeroGradient;
    }
    outside
    {
        type            zeroGradient;
    }
    sides
    {
        type            empty;
    }
}


